   000522                               WS-CMPT-B - WS-CMPT-PRINCIPAL-AMT.                         93 102
   000523
   000524                    COMPUTE WS-TOTAL-PAYMENTS =                                           116
   000525                                   WS-TOTAL-PAYMENTS + WS-CMPT-MNTHLY-PMT.                116 97
   000526
   000527                    COMPUTE WS-TOTAL-INTEREST =                                           117
   000528                                   WS-TOTAL-INTEREST + WS-CMPT-INTEREST-DUE.              117 100
   000529
   000530                    COMPUTE WS-TOTAL-PRINCIPL =                                           118
   000531                                   WS-TOTAL-PRINCIPL + WS-CMPT-PRINCIPAL-AMT.             118 102
   000532
