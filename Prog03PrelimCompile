   000337                32000-VALIDATE-INPUT-DATA.
   000338
   000339                    EVALUATE LOAN-TYPE-VAL                                                34
   000340      1                WHEN '100N' MOVE  'NEW CAR'          TO WS-CMPT-LOAN-NAME          84
   000341      1                WHEN '100U' MOVE  'USED CAR'         TO WS-CMPT-LOAN-NAME          84
   000342      1                WHEN '200'  MOVE  'BOAT'             TO WS-CMPT-LOAN-NAME          84
   000343      1                WHEN '300'  MOVE  'SIGNATURE LOAN'   TO WS-CMPT-LOAN-NAME          84
   000344      1                WHEN '400'  MOVE  'R/V'              TO WS-CMPT-LOAN-NAME          84
   000345      1                WHEN '500'  MOVE  'MOTORCYCLE'       TO WS-CMPT-LOAN-NAME          84
   000346      1                WHEN '600'  MOVE  'MORTGAGE'         TO WS-CMPT-LOAN-NAME          84
   000347      1                WHEN '700'  MOVE  'HOME EQUITY'      TO WS-CMPT-LOAN-NAME          84
   000348      1                WHEN '800'  MOVE  'HOME IMPROVEMENT' TO WS-CMPT-LOAN-NAME          84
   000349      1                WHEN OTHER MOVE 'Y' TO WS-EXCEPTION-LOAN-TYPE,                     75
   000350      1                                       WS-EXCEPTION-CHECK                          79
   000351                    END-EVALUATE.
   000352
   000353
   000354                    IF PRINCIPAL-AMT > 0                                                  35
   000355               *       IF PRINCIPAL-AMT IS NUMERIC
   000356      1                  MOVE PRINCIPAL-AMT TO WS-CMPT-PRINCIPAL                          35 87
   000357                       ELSE
   000358      1                  MOVE 'Y' TO WS-EXCEPTION-PRINCIPAL, WS-EXCEPTION-CHECK           76 79
   000359               *       END-IF
   000360                    END-IF.
   000361
   000362
   000363                    IF TERM-IN-YEARS > 0                                                  37
   000364      1                IF TERM-IN-YEARS IS NUMERIC                                        37
   000365      2                  MOVE TERM-IN-YEARS TO WS-CMPT-TERM-IN-YEARS                      37 88
   000366      1             ELSE
   000367      2                  MOVE 'Y' TO WS-EXCEPTION-TERM-YEAR, WS-EXCEPTION-CHECK           77 79
   000368      1                  END-IF
   000369                    END-IF.
   000370
   000371
   000372                    INSPECT ANNUAL-PCT-RT TALLYING WS-DECIMAL-COUNT FOR ALL '.'.          39 80
   000373                    IF WS-DECIMAL-COUNT = 0                                               80
   000374      1                  MOVE ANNUAL-PCT-RT TO WS-CMPT-ANNUAL-PCT-RT                      39 85
   000375                    ELSE
   000376      1                  MOVE 'Y' TO WS-EXCEPTION-ANNUAL-PR, WS-EXCEPTION-CHECK           78 79
   000377                    END-IF.
   000378                    MOVE 0 TO WS-DECIMAL-COUNT.                                           80
   000379                    IF ANNUAL-PCT-RT = 0                                                  39
   000380      1                  MOVE 'Y' TO WS-EXCEPTION-ANNUAL-PR, WS-EXCEPTION-CHECK           78 79
   000381                    END-IF.
   000382
   000383                    IF WS-EXCEPTION-CHECK = 'Y'                                           79
   000384      1                   PERFORM 33000-PROCESS-EXCEPTION                                 396
   000385      1                   MOVE 'N' TO WS-EXCEPTION-CHECK                                  79
   000386                    ELSE
   000387      1                   PERFORM 34000-COMPUTE-SCHEDULE-DATA                             428
   000388                    END-IF.
